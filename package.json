{
  "name": "mfa-store",
  "version": "0.0.1",
  "scripts": {
    "ng":"ng",
    "start": "concurrently \"npm run start:store\" \"npm run start:order-management\" \"npm run start:service-management\"",
    "start:store": "ng serve --project=store --port 4200",
    "start:order-management": "ng serve --project=order-management --port 4300",
    "start:service-management": "ng serve --project=service-management --port 4400",
    "build": "npm run build:store && npm run build:order-management && npm run build:service-management",
    "build:store": "ng build --project=store",
    "build:order-management": "ng build --project=order-management",
    "build:service-management": "ng build --project=service-management",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
